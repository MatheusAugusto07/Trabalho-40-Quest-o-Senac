int numero, resultado;

Console.Write("Digite um n√∫mero para ver a tabuada: ");
numero = Convert.ToInt32(Console.ReadLine());

Console.WriteLine("Tabuada do " + numero + ":");

for (int i = 1; i <= 10; i++)
{
    resultado = numero * i;
    Console.WriteLine(numero + " x " + i + " = " + resultado);
}
